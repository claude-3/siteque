{
    "manifest_version": 3,
    "name": "SiteCue",
    "version": "0.1.0",
    "description": "Context-Aware Layer Extension",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "SiteCue"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}